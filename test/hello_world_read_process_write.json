{
    /*
     * simple ETL: read from a CSV, reorder the columns and then write data to a CSV
     */

    "metadata": {
        "section_registry": [
            "reader_config",
            "pipeline_config",
            "writer_config"
        ],
        "class_package": [
            "./src",
            "./test/sample_project"
        ],
    },
     "implementation_config": {
        "reader_config": {
            "read_data": {
                "class": "CsvReader",
                "filename": "data/tennis.csv",
                "destinations": [
                    "reorder_cols"
                ]
            }
        },
        "pipeline_config": {
            "reorder_cols": {
                "class": "ColumnReorder",
                "cols_order": [
                    "id",
                    "outlook",
                    "humidity",
                    "play",
                    "temp",
                    "windy"
                ],
                "destinations": [
                    "write_output"
                ]
            }
        },
        "writer_config": {
            "write_output": {
                "class": "CsvWriter",
                "key": "data",
                "dir": "test/sample_project/data",
                "filename": "tennis_output.csv"
            }
        }
    }
}

